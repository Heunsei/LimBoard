model User {
    id Int @id @default(autoincrement())
    name String
    email String @unique
    createdAt DateTime
    team TeamsOnUsers[]
    task Task[]
}

model TeamsOnUsers {
    team Team @relation(fields: [teamId], references: [id])
    teamId Int
    user User @relation(fields: [userId], references: [id])
    userId Int
    @@id([teamId, userId])
}